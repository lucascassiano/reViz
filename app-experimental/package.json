{
  "name": "reviz3d-experimental",
  "version": "1.0.0",
  "description": "",
  "main": "./engine/index.js",
  "scripts": {
    "prestart": "rimraf ./engine/app && npm run parcel:build",
    "parcel:build": "parcel build ./src/* --target=electron --out-dir ./engine/app --public-url ./",
    "parcel:start": "parcel watch ./src/* --target=electron --out-dir ./engine/app --public-url ./",
    "electron:start": "NODE_ENV=development electron -r babel-register ./engine/",
    "dev": "RELOAD=true npm start",
    "start": "run-p -l parcel:start electron:start",
    "postinstall": "electron-rebuild"
  },
  "author": "lucascassiano",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-0": "^6.24.1",
    "electron": "^3.0.0-beta.8",
    "electron-is-dev": "^0.3.0",
    "electron-reload": "^1.2.5",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "rimraf": "^2.6.2",
    "serialport": "^7.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "electron-rebuild": "^1.8.2",
    "parcel-bundler": "^1.9.7",
    "svg-react-loader": "^0.4.5"
  }
}
