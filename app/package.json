{
    "name": "reViz3d",
    "version": "1.1.0",
    "author": "Lucas Cassiano Silva",
    "homepage": "./",
    "description": "",
    "main": "public/electron.js",
    "repository": "https://github.com/Lucas Cassiano Silva/app4",
    "build": {
        "appId": "com.app4",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets",
            "output": "../dist"
        },
        "mac": {
            "target": "zip",
            "icon": "icon.icns"
        },
        "win": {
            "target": "zip",
            "icon": "icon.ico"
        }
    },
    "dependencies": {
        "@blueprintjs/core": "^1.34.1",
        "@vimeo/player": "^2.2.1",
        "array-to-image": "^1.0.0",
        "axios": "^0.17.1",
        "bootstrap": "^4.0.0-beta.2",
        "brace": "^0.11.0",
        "colby-wp-react-vimeo-player": "^1.0.4",
        "electron-is-dev": "^0.3.0",
        "electron-updater": "^2.16.3",
        "get-pixels": "^3.3.0",
        "loaders.css": "^0.1.2",
        "mapbox": "^1.0.0-beta9",
        "moment": "^2.20.1",
        "mqtt": "^2.15.0",
        "node-dir": "^0.1.17",
        "node-watch": "^0.5.5",
        "object-sizeof": "^1.2.0",
        "pngparse": "^2.0.1",
        "project-version": "^1.0.0",
        "react": "^15.6.2",
        "react-ace": "^5.8.0",
        "react-addons-css-transition-group": "^15.6.2",
        "react-alert": "^2.4.0",
        "react-console": "0.0.8",
        "react-container-3d": "^0.5.6",
        "react-cubeview": "^2.6.23",
        "react-dom": "^15.6.2",
        "react-downloadbutton": "^1.0.0",
        "react-draggable": "^3.0.4",
        "react-drawer": "^1.2.5",
        "react-dropzone": "^4.2.3",
        "react-json-tree": "^0.11.0",
        "react-json-view": "^1.16.0",
        "react-loaders": "^3.0.1",
        "react-menu-bar": "^2.0.4",
        "react-minimalist-toolbar": "^1.0.6",
        "react-monaco-editor": "^0.12.0",
        "react-p5-wrapper": "0.0.4",
        "react-player": "^1.1.1",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.0.10",
        "react-tabs": "^2.1.1",
        "react-text-input": "0.0.8",
        "react-treeview": "^0.4.7",
        "react-ui-tree": "^3.1.0",
        "react-vimeo": "^1.0.0",
        "reactstrap": "^5.0.0-alpha.4",
        "redux": "^3.7.2",
        "semantic-ui-css": "^2.2.12",
        "semantic-ui-react": "^0.77.0",
        "serialport": "^6.0.1",
        "three-react-obj-loader": "^1.0.4",
        "three-transform-controls": "^1.0.4",
        "three-transformcontrols": "^0.78.1",
        "uikit-react": "^2.3.8",
        "video-react": "^0.9.4"
    },
    "scripts": {
        "preship": "yarn build",
        "ship": "build --em.main=build/electron.js --win --ia32 -p --darwin always -c electron-builder.yml",
        "start": "NODE_ENV=development_browser react-scripts start ",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "dev": "concurrently \"export BROWSER=true && yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "build-win": "npm run build && build --em.main=build/electron.js --win && electron-rebuild .",
        "build-linux": "npm run build && build --em.main=build/electron.js --linux ./electron-builder.yml && electron-rebuild .",
        "build-mac": "npm run build && build --em.main=build/electron.js --mac ./electron-builder.yml && electron-rebuild .",
        "install": "electron-rebuild",
        "export": "build --em.main=build/electron.js ",
        "build-all": "npm run build && build --em.main=build/electron.js --win --ia32 --mac./electron-builder.yml && electron-rebuild .",
        "postinstall": "electron-builder install-app-deps"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "concurrently": "^3.5.0",
        "electron": "^1.7.5",
        "electron-builder": "^19.20.1",
        "electron-rebuild": "^1.6.0",
        "wait-on": "^2.0.2"
    }
}